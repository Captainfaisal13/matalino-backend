{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "slug": {
      "type": "uid"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    },
    "price": {
      "type": "integer"
    },
    "salePrice": {
      "type": "integer"
    },
    "inStock": {
      "type": "boolean"
    },
    "sizes": {
      "type": "enumeration",
      "enum": [
        "S",
        "M",
        "L",
        "XL",
        "XXL"
      ]
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "featured": {
      "type": "boolean"
    }
  }
}
